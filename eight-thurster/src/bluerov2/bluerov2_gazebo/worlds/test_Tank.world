<?xml version="1.0" ?>
<!-- Test Tank world for BlueROV2 simulation
     Tank dimensions: 18.15m x 2.5m x 1m
-->
<sdf version="1.4">
  <world name="test_Tank">
    <physics name="default_physics" default="true" type="ode">
      <max_step_size>0.002</max_step_size>
      <real_time_factor>1</real_time_factor>
      <real_time_update_rate>500</real_time_update_rate>
      <ode>
        <solver>
          <type>quick</type>
          <iters>50</iters>
          <sor>1.2</sor>
        </solver>
      </ode>
    </physics>
    
    <scene>
      <ambient>0.01 0.01 0.01 1.0</ambient>
      <sky>
        <clouds>
          <speed>12</speed>
        </clouds>
      </sky>
      <shadows>1</shadows>
      <fog>
        <color>0.1 0.2 0.3 1.0</color>
        <type>linear</type>
        <density>0.1</density>
        <start>5</start>
        <end>25</end>
      </fog>
    </scene>

    <!-- Origin coordinates -->
    <spherical_coordinates>
      <latitude_deg>56.71897669633431</latitude_deg>
      <longitude_deg>3.515625</longitude_deg>
    </spherical_coordinates>

    <!-- Global light source -->
    <light type="directional" name="sun1">
      <pose>15 0 10 0 0 0</pose>
      <diffuse>1 1 1 1</diffuse>
      <specular>.1 .1 .1 1</specular>
      <direction>0.3 0.3 -1</direction>
      <cast_shadows>false</cast_shadows>
    </light>

    <!-- Global light source -->
    <light type="directional" name="sun_diffuse">
      <pose>-15 0 -10 0 0 0</pose>
      <diffuse>0.6 0.6 0.6 1</diffuse>
      <specular>0 0 0 1</specular>
      <direction>-0.3 -0.3 -1</direction>
      <cast_shadows>false</cast_shadows>
    </light>

    <!-- Virtual NED frame -->
    <include>
      <uri>model://ned_frame</uri>
      <pose>0 0 0 0 0 0</pose>
    </include>

    <!-- Custom tank model -->
    <model name="custom_tank">
      <static>true</static>
      <link name="tank_link">
        
        <!-- Water surface -->
        <visual name="water_surface">
          <cast_shadows>false</cast_shadows>
          <pose>0 0 0 0 0 0</pose>
          <geometry>
            <box>
              <size>18.15 2.5 0.01</size>
            </box>
          </geometry>
          <material>
            <script>
              <uri>file://Media/materials/scripts/water.material</uri>
              <name>UUVSimulator/StaticDeepWater</name>
            </script>
          </material>
        </visual>

        <!-- Tank bottom -->
        <visual name="tank_bottom">
          <cast_shadows>false</cast_shadows>
          <pose>0 0 -1 0 0 0</pose>
          <geometry>
            <box>
              <size>18.15 2.5 0.05</size>
            </box>
          </geometry>
          <material>
            <script>
              <uri>file://Media/materials/scripts/sand.material</uri>
              <name>UUVSimulator/SandAndStones</name>
            </script>
          </material>
        </visual>

        <collision name="tank_bottom">
          <pose>0 0 -1 0 0 0</pose>
          <geometry>
            <box>
              <size>18.15 2.5 0.05</size>
            </box>
          </geometry>
        </collision>

        <!-- Tank walls - North (positive X) -->
        <visual name="wall_north">
          <cast_shadows>false</cast_shadows>
          <pose>9.075 0 -0.5 0 0 0</pose>
          <geometry>
            <box>
              <size>0.05 2.5 1</size>
            </box>
          </geometry>
          <material>
            <script>
              <uri>file://Media/materials/scripts/water.material</uri>
              <name>UUVSimulator/SeaWall</name>
            </script>
          </material>
        </visual>

        <collision name="wall_north">
          <pose>9.075 0 -0.5 0 0 0</pose>
          <geometry>
            <box>
              <size>0.05 2.5 1</size>
            </box>
          </geometry>
        </collision>

        <!-- Tank walls - South (negative X) -->
        <visual name="wall_south">
          <cast_shadows>false</cast_shadows>
          <pose>-9.075 0 -0.5 0 0 0</pose>
          <geometry>
            <box>
              <size>0.05 2.5 1</size>
            </box>
          </geometry>
          <material>
            <script>
              <uri>file://Media/materials/scripts/water.material</uri>
              <name>UUVSimulator/SeaWall</name>
            </script>
          </material>
        </visual>

        <collision name="wall_south">
          <pose>-9.075 0 -0.5 0 0 0</pose>
          <geometry>
            <box>
              <size>0.05 2.5 1</size>
            </box>
          </geometry>
        </collision>

        <!-- Tank walls - East (positive Y) -->
        <visual name="wall_east">
          <cast_shadows>false</cast_shadows>
          <pose>0 1.25 -0.5 0 0 0</pose>
          <geometry>
            <box>
              <size>18.15 0.05 1</size>
            </box>
          </geometry>
          <material>
            <script>
              <uri>file://Media/materials/scripts/water.material</uri>
              <name>UUVSimulator/SeaWall</name>
            </script>
          </material>
        </visual>

        <collision name="wall_east">
          <pose>0 1.25 -0.5 0 0 0</pose>
          <geometry>
            <box>
              <size>18.15 0.05 1</size>
            </box>
          </geometry>
        </collision>

        <!-- Tank walls - West (negative Y) -->
        <visual name="wall_west">
          <cast_shadows>false</cast_shadows>
          <pose>0 -1.25 -0.5 0 0 0</pose>
          <geometry>
            <box>
              <size>18.15 0.05 1</size>
            </box>
          </geometry>
          <material>
            <script>
              <uri>file://Media/materials/scripts/water.material</uri>
              <name>UUVSimulator/SeaWall</name>
            </script>
          </material>
        </visual>

        <collision name="wall_west">
          <pose>0 -1.25 -0.5 0 0 0</pose>
          <geometry>
            <box>
              <size>18.15 0.05 1</size>
            </box>
          </geometry>
        </collision>

      </link>
    </model>

    <!-- Underwater current plugin -->
    <plugin name="underwater_current_plugin" filename="libuuv_underwater_current_ros_plugin.so">
      <namespace>hydrodynamics</namespace>
      <constant_current>
        <topic>current_velocity</topic>
        <velocity>
          <mean>0</mean>
          <min>0</min>
          <max>2</max>
          <mu>0.0</mu>
          <noiseAmp>0.0</noiseAmp>
        </velocity>

        <horizontal_angle>
          <mean>0</mean>
          <min>-3.141592653589793238</min>
          <max>3.141592653589793238</max>
          <mu>0.0</mu>
          <noiseAmp>0.0</noiseAmp>
        </horizontal_angle>

        <vertical_angle>
          <mean>0</mean>
          <min>-3.141592653589793238</min>
          <max>3.141592653589793238</max>
          <mu>0.0</mu>
          <noiseAmp>0.0</noiseAmp>
        </vertical_angle>
      </constant_current>
    </plugin>

    <plugin name="sc_interface" filename="libuuv_sc_ros_interface_plugin.so"/>

    <gui fullscreen='0'>
      <camera name='user_camera'>
        <pose frame=''>15 2 -0.5 0 0.2 3.14</pose>
        <view_controller>orbit</view_controller>
        <projection_type>perspective</projection_type>
      </camera>
    </gui>
  </world>
</sdf> 